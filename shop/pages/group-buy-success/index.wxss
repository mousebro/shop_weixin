/* pages/group-buy-success/index.wxss */
page{background: #f2f2f2;}
.all-container{margin-top:12rpx;}
.buttonFixed{position: fixed;bottom:19rpx;width: 96%;margin:0 2%;background:linear-gradient(to right,#ff3751,#ff8675);text-align: center;color: #fff;font-size:35rpx;padding:32rpx;box-sizing: border-box;border-radius: 3rpx; }
.box-container{width: 96%;margin:0 2%;}
.product-item{padding:50rpx 30rpx;background:#fff;display: flex;justify-content: space-between;}
.product-item .product-img{width:170rpx;height:120rpx;}
.product-msg-container{width:calc(100% - 170rpx);padding: 0;margin-top:-50rpx;}
.product-msg-container .title{font-size:30rpx;}
.product-msg-container .count-container{display: flex;justify-content: space-between;}
.product-msg-container .count-container .count-total{font-size:24rpx;color:#777777;}
.product-msg-container .count-container .count-buy{font-size:27rpx;color:#555555;}
.price-container{display: flex;justify-content:space-between;}
.price-container .count{background:linear-gradient(to right,#ff3751,#ff8675);color:#fff;padding:2rpx 20rpx;font-size:24rpx;border-radius:30rpx;line-height:32rpx;height:32rpx; }
.price-container .price .oldPrice{font-size:20rpx;text-decoration: line-through;margin-right: 10rpx;vertical-align: middle;color:#777777;  }
.price-container .price .newPrice{font-size:40rpx;color:#ea4149; }
.price-container .price .newPrice02{font-size:40rpx;color:#ea4149;font-size:25rpx; }
.count-container{margin-top:24rpx}
.price-container{margin-top:24rpx}

/**拼团成员*/
.team-container{margin-top:12rpx;background: #fff;padding:100rpx 150rpx;}
.team-container .team-msg{text-align: center;color:#ea4149;}
.team-container .team-msg::before{display:inline-block;margin-right:4%;width: 20%;content: "";height: 0;border-top: 1px solid #ea4149;vertical-align: middle;} 
.team-container .team-msg::after{display:inline-block;margin-left:4%;width: 20%;content: "";height: 0;border-top: 1px solid #ea4149;vertical-align: middle;}
.team-container .team-msg-two>view:first-child{font-size: 30rpx;text-align: center;}
.team-container .team-msg-two>view>text{color:#ea4149;}
.team-container .team-msg-two>view:nth-child(2){text-align: center;margin-top:40rpx;}
.team-container .team-msg-two>view:nth-child(2)>text{color:#fff;background:#8f8f8f;padding:6rpx 10rpx;font-size:34rpx;margin:10rpx;border-radius:5rpx;}
.list-item-avator-container{display: flex;justify-content: space-around;padding:10%}
.avator{width:80rpx;height:80rpx;border-radius:150rpx;margin:0 25rpx;}
.list-item-avator-container view{position: relative;}
.list-item-avator-container view view{position: absolute;font-size:16rpx;background:linear-gradient(to right,#ff3751,#ff8675);color:#fff;top:-10rpx;left:calc(50% - 20rpx);display: inline-block;padding:4rpx 8rpx;height:24rpx;border-radius:12rpx;}