  /* @import "/wxParse/wxParse.wxss"; */
  page{overflow: hidden;}
  .clearfix:after {content: ".";display: block;height: 0;clear: both;visibility: hidden;}
  button{padding: 0;font-size: 36rpx;line-height: 50rpx;overflow: visible;background-color: #ffffff;}
  button::after{border: none;}
  .wxParse-img{display: flex;}
  .shop-title{width: 90%;padding:0 5%;height:1rpx;line-height: 80rpx;box-shadow: 0rpx 4rpx 8rpx #e5e5e5;position:relative;z-index: 5;margin-top: 40rpx;}
  .shop-title text{display: block;width: 33.3%;float: left;text-align: center;font-size: 30rpx;}
  .shop-title-active{color: #ea4149;}

  .scroll-body{height: calc(100vh)}
  .shop-swiper{width: 100%;height: 700rpx;border-bottom: 1px solid #e8e8e8;position:relative;z-index: 2;overflow: hidden;}
  .swiper-img{width: 100%;height: 100%;}
  .swiper-img-avatar{width: 60rpx;height: 60rpx;border-radius:60rpx;margin-right: 10rpx;background: #fff;}
  .shop-swiper-custom{height:100rpx;background:#f2f2f2;}
  .shop-swiper-item{width: 35%;}
  .shop-swiper-custom view{display: flex;align-items: center;width:100%;font-size:28rpx;color:#222222;height:100%;padding:0 40rpx;}
  

  .shop-detail{width: 90%;padding: 0 5% 30rpx;}
  .shop-detail-name{width: 100%;position: relative;}
  .shop-detail-name > text{width: 100%;display: block;line-height: 60rpx;font-size:32rpx;margin-top: 20rpx;}
  .shop-detail-share{position: fixed;right: 0;top:80rpx;background-color: #ea4149;border-radius: 40rpx 0 0 40rpx;padding: 10rpx 20rpx 10rpx 10rpx;height: 36rpx;line-height: 36rpx;z-index:19;}
  .shop-detail-share image{width: 30rpx;height: 30rpx;display: block;float: left;margin-right: 4rpx;margin-top: 2rpx;}
  .shop-detail-share button{font-size: 24rpx;color: #ffffff;display: block;float: left;background: none;margin-top: -6rpx;}

  .shop-detail-custom{position: absolute;right: -6%;top:0rpx;padding: 10rpx 20rpx 10rpx 10rpx;height: 36rpx;line-height: 36rpx;}
  .shop-detail-custom image{width: 30rpx;height: 30rpx;display: block;float: left;margin-right: 4rpx;margin-top: 2rpx;}
  .shop-detail-custom button{font-size: 30rpx;display: block;float: left;background: none;margin-top: -6rpx;color:#ea4149;}
  .shop-detail-price{width: 100%;position: relative;}
  .shop-detail-price text.price-icon{font-size: 26rpx;color: #ea4149;}
 
  .shop-detail-price text:nth-child(3){font-size: 30rpx;color: #ea4149;}
  .shop-detail-price text.marketprice{font-size: 40rpx;color: #ea4149;}
  .shop-detail-price text.delLine{font-size: 24rpx;color:#8a8a8a;text-decoration:line-through;margin-left: 20rpx;}
  .shop-detail-super{width: 100%;height: 60rpx;line-height: 60rpx;margin-bottom: 30rpx;}
  .shop-detail-super image{width: 120rpx;margin-bottom: -8rpx}
  .shop-detail-super text{margin-left:20rpx;font-size: 24rpx;}

  .shop-assess{width: 90%;padding: 0 5%;border-top: 30rpx solid #f2f2f2;margin-bottom: 20rpx;}
  .shop-assess-title{width: 100%;height: 50rpx;line-height: 50rpx;margin-top: 30rpx;}
  .shop-assess-title text:nth-child(1){font-size: 32rpx;color: #222222;display: block;width: 50%;text-align: left;float: left;}
  .shop-assess-title text:nth-child(2){font-size: 24rpx;color: #999999;display: block;width: 50%;text-align: right;float: left;}
  .shop-assess-info{width: 100%;height:80rpx;line-height: 80rpx;}
  .shop-assess-info image{width: 50rpx;height: 50rpx;border-radius: 50%;margin-bottom: -14rpx;}
  .shop-assess-info text{font-size: 26rpx;color:#999999;margin-left: 10rpx;}
  .shop-assess-detail{width: 100%;}
  .shop-assess-detail image{width: 30%;padding:0 1.6%;height:200rpx;float: left;margin-top: 10rpx;}
  .shop-assess-detail text{display: block;font-size: 28rpx;line-height: 40rpx;width: 100%;}

  .shop-parse{width: 90%;padding: 0 5%;}
  .shop-parse text:nth-child(1){text-align: center;position: relative;display: block;width: 100%;margin: 40rpx 0;font-size: 32rpx}
  .shop-parse text:nth-child(1)::before{content: '';position: absolute;left: 140rpx;top: 24rpx;width: 100rpx;height: 2rpx;background-color: #e2e2e2;}
  .shop-parse text:nth-child(1)::after{content: '';position: absolute;right: 140rpx;top: 24rpx;width: 100rpx;height: 2rpx;background-color: #e2e2e2;}

  .shop-buy{width: 100%;height: 100rpx;position: fixed;bottom: 20rpx;z-index: 10;}
  /* .shop-buy-now{width: 80%;height: 80rpx;line-height: 100rpx;color: #ffffff;text-align: center;font-size: 40rpx;background:linear-gradient(to right,#ea4149,#ff6849);} */

  .shop-buy-now{width:85%;animation: free_download 1s linear alternate infinite;-webkit-animation: free_download 1s linear alternate infinite;
    height: 100rpx;line-height: 100rpx;color: #ffffff;text-align: center;font-size: 32rpx;background:linear-gradient(to right,#ea4149,#ff6849);margin:0 auto 20rpx;border-radius: 20rpx;
  }
  .shop-buy-now>.bargain-btn{height: 100rpx;line-height: 100rpx;color:#fff;background:linear-gradient(to right,#ea4149,#ff6849);border-radius: 20rpx;}
@-webkit-keyframes free_download{0%{-webkit-transform:scale(1);}100%{-webkit-transform:scale(1.17);}}
@keyframes free_download{0%{transform:scale(1);}100%{transform:scale(1.17);}}

  .mask{width: 100%;height: 100%;position: fixed;top: 0;left: 0;background: #000000;z-index: 20;opacity: 0.6;}
  .modal{width: 100%;position:fixed;bottom: 100rpx;background-color: #ffffff;z-index: 21;}
  .modal-shopDetail{width: 90%;margin: 20rpx 5%;position: relative;padding-bottom: 20rpx;border-bottom: 1px solid #e2e2e2;}
  .modal-shopDetail image:nth-child(1){width: 120rpx;height: 120rpx;display: block;float: left;}
  .modal-shopDetail image:nth-child(4){position: absolute;right: 0;top: 0;width: 40rpx;height: 40rpx;}
  .modal-shopDetail text{display: block;width: 60%;font-size: 30rpx;float: left;margin-left: 20rpx;line-height: 60rpx;}
  .modal-shopDetail text:nth-child(3){color: #ea4149;}
  .modal-bottomButton{width: 100%;position: fixed;bottom: 0;height: 100rpx;line-height: 100rpx;background-color: #ea4149;color: #fff;font-size: 30rpx;text-align: center;z-index: 21;}

  .modal-shopCount{display: flex;justify-content: space-between;width: 90%;margin-left: 5%;margin-bottom: 20rpx;align-items: center;}
  .modal-shopCount-number{font-size:26rpx;color:#000000;}
  .modal-shopCount .modal-shopCount-box{display: flex;}
  .modal-shopCount .modal-shopCount-box .modal-shopCount-less,.modal-shopCount .modal-shopCount-box .modal-shopCount-input,.modal-shopCount .modal-shopCount-box .modal-shopCount-plus{width: 80rpx;height: 64rpx;line-height: 62rpx;text-align: center;border: 1px solid #eee;}
  .modal-shopCount .modal-shopCount-box .modal-shopCount-input{font-size: 28rpx;}
  .modal-shopCount .modal-shopCount-box .modal-shopCount-input input{height: 100%;}

  .hui{background-color: #f5f5f9;}
  .isIpx{padding-bottom: 68rpx;}
  .swiper-index{position:absolute;top:60rpx;left:20rpx;z-index:10;width:100rpx;height:100rpx;}

  .inline-view{display: inline-block;}
  .block-view{display: block;}

  /*弹出窗商品规格*/
.sort-size{padding-bottom:20rpx;margin:20rpx 5%;width:90%;border-bottom:1px solid #e2e2e2}
.sort-size>.sort-size-title{margin-bottom:20rpx;}
.sort-size>.sort-list{display: flex;justify-content:flex-start;flex-wrap: wrap;}
.sort-size>.sort-list>text{display: inline-block;border: 1px solid #e2e2e2;color: #222222;text-align: center;margin:1% 4% 2% 0;border-radius:10rpx;font-size:24rpx;padding:6rpx 10rpx;}
.sort-size>.sort-list>text.active{border:1px solid #ea4149;color:#ea4149;}


.sheet-container{background: transparent;width: 100%;}
.sheet-item{background: transparent;border-radius: 10rpx;font-size:36rpx;}
.sheet-button{background: transparent;}

/*隐藏scroll-view的滚动条*/
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  }
