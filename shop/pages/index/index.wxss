/**index.wxss**/
page{background-color: #f2f2f2;position: relative;}
.body{margin-top: 98rpx;}
.hasnotop{margin-top:0;}
button{padding: 0;font-size: 36rpx;line-height: 50rpx;overflow: visible;background-color: #ffffff;}
.buttton-hover{background: none;}
button::after{border: none;}
.clearfix:after {content: " ";display: block;height: 0;clear: both;visibility: hidden;}
.index-title{width: 100%;background: linear-gradient(#f85b49, #ff6849);position: fixed;top: 0;z-index: 20;border-bottom:1px solid #ff6849;}
/* 搜索 */
.index-search{width: 80%;background-color: #ffffff;border-radius: 40rpx;height: 70rpx;line-height: 70rpx;margin: 20rpx 2% 20rpx 5%;float: left;}
.index-search image{width: 40rpx;height: 40rpx;margin-left: 20rpx;display: inline-block;margin-bottom: -10rpx;}
.index-search text{font-size: 26rpx;margin-left: 10rpx;color: #777777;display: inline-block;}
/* 客服 */
.index-sever{width:10%;float: left;height: auto;text-align: center;margin-top: 20rpx}
.index-sever image{width: 44rpx;height: 40rpx;}
.index-sever text{display: block;width: 100%;font-size: 24rpx;margin-top: -10rpx;color: #ffffff;}
/* banner */
.index-swiper{width: 100%;height: 320rpx;}
.index-swiper-block{width: 100%;height: 100%;}
.swiper-img{width: 100%;height: 100%;}
/* 自定义入口 */
.index-entrance{width: 96%;padding:20rpx 2%;background-color: #ffffff;}
.index-entrance-block{width: 20%;padding:10rpx 2.5%;text-align: center;float: left;}
.index-entrance-block image{width: 85%;height: 120rpx;border-radius: 50%;}
.index-entrance-block text{display: block;width: 100%;font-size: 26rpx;}
/* 新人福利 */
.index-newuser{width: 96%;padding:20rpx 2% 0;background-color: #ffffff;}
.index-newuser image{width: 100%;}
/* 人人拼团 */
.index-group{width: 100%;background-color: #ffffff;}
.index-group-title{padding: 10rpx 4%;width: 92%;}
.index-group-title text:nth-child(1){font-size: 32rpx;font-weight: bold;display: block;float: left;width: 50%;line-height: 50rpx;}
.index-group-title text:nth-child(2){font-size: 24rpx;display: block;float: left;width: 50%;color: #777777;text-align: right;line-height: 50rpx;}
.index-group > swiper{margin-top: 20rpx;height: 580rpx;}
.index-group-img{width: 100%;height: 380rpx;border-radius: 10rpx;}
.index-group-block swiper-item{width: 94% !important;padding:0 3%}
.index-group-block swiper-item > text{display: block;width: 100%;font-size: 26rpx;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;padding: 20rpx 0;}
.index-group-info{margin-top: 20rpx;}
.index-group-info text:nth-child(1){font-size: 24rpx;color: #ea4149;}
.index-group-info text:nth-child(2){font-size: 42rpx;color: #ea4149;font-weight: bold;}
.index-group-info text:nth-child(3){font-size: 24rpx;color: #555555;margin-left: 20rpx;}
.index-group-info text:nth-child(4){font-size: 24rpx;color: #ea4149;}
.index-group-info text:nth-child(5){font-size: 24rpx;color: #555555;}
.index-group-info text:nth-child(6){display: block;height: 40rpx;padding: 10rpx 20rpx;line-height: 40rpx;font-size: 30rpx;color: #ffffff;float: right;background-color:#ff5559;border: 1px solid #ff5559;border-radius: 6rpx;}
.index-group-info text:nth-child(7){display: block;height: 40rpx;padding: 10rpx 20rpx;line-height: 40rpx;font-size: 26rpx;color: #ff5559;float: right;border: 1px solid #ff5559;border-radius: 6rpx;margin-right: -10rpx;}
.index-group-info image{float: right;width: 50rpx;height: 50rpx;border-radius: 50%;border: 1px solid #ffffff;margin-right: 20rpx;position: relative;z-index: 10}
.index-group-info image:nth-child(9){margin-right: -10rpx;position: relative;z-index: 9}
.index-group-info image:nth-child(10){margin-right: -10rpx;position: relative;z-index: 8}
/* 掌上秒拍 */
.index-seckill{width: 100%;background-color: #ffffff;margin-top: 20rpx;}
.index-seckill-title{width: 100%;padding: 10rpx 4%;width: 92%;position: relative;border-bottom: 1px solid #e8e8e8;height: 100rpx;line-height: 100rpx;}
.index-seckill-title text:nth-child(1){font-size: 32rpx;font-weight: bold;}
.index-seckill-title text:nth-child(2){font-size: 26rpx;margin-left: 30rpx;}
.index-seckill-title text:nth-child(3){font-size: 26rpx;background-color: #8f8f8f;color: #ffffff;padding: 4rpx 6rpx;border-radius: 4rpx;margin: 0 4rpx;}
.index-seckill-title text:nth-child(4){font-size: 26rpx;}
.index-seckill-title text:nth-child(5){font-size: 26rpx;background-color: #8f8f8f;color: #ffffff;padding: 4rpx 6rpx;border-radius: 4rpx;margin: 0 4rpx;}
.index-seckill-title text:nth-child(6){font-size: 26rpx;}
.index-seckill-title text:nth-child(7){font-size: 26rpx;background-color: #8f8f8f;color: #ffffff;padding: 4rpx 6rpx;border-radius: 4rpx;margin: 0 4rpx;}
.index-seckill-title text:nth-child(8){font-size: 26rpx;color:#8f8f8f;position: absolute;right: 50rpx;top: 10rpx;}
.index-seckill-title .isDone{font-size: 26rpx;color:#8f8f8f !important;background-color: #fff !important;position: absolute;right: 50rpx;top: 10rpx;}
.index-seckill-title > image{width: 16rpx;height: 20rpx;position: absolute;right: 30rpx;top: 50rpx;}

.index-seckill-body{width: 92%;white-space:nowrap;padding: 20rpx 4%;}
.index-seckill-body-block{width: 25%;display: inline-block;flex: auto;margin-right: 4%;}
.index-seckill-body-block image{width: 100%;height: 180rpx;}
.index-seckill-body-block text{display: block;width: 100%;}
.index-seckill-body-block text:nth-child(2){font-size: 30rpx;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.index-seckill-body-block text:nth-child(3){font-size: 26rpx;color: #ea4149;}
.index-seckill-body-block text:nth-child(4){font-size: 24rpx;color: #777777;text-decoration:line-through;}
/* 魔方 */
.cubeList>.cubeItem:not(:last-child){margin-bottom: 20rpx;}
.cubeList>.cubeItem:nth-child(2) .index-cubeTitle{background: linear-gradient(to right,#f6cb99, #f8e7ad);}
.cubeList>.cubeItem:nth-child(3) .index-cubeTitle{background: linear-gradient(to right,#cfe7fc, #c5cbfa);}
.cubeList>.cubeItem:nth-child(4) .index-cubeTitle{background: linear-gradient(to right,#bddfeb, #c3f1e9);}
.cubeList>.cubeItem:nth-child(5 ) .index-cubeTitle{background: linear-gradient(to right,#f6cb99, #f8e7ad);}
.index-cubeTitle{width: calc(100% - 40rpx);height: 100rpx;background: linear-gradient(to right,#f5c8df, #f7cecd);display: flex;justify-content: space-between;padding:40rpx 20rpx;}
.index-cubeTitle>.cube-title{font-size:32rpx;color:#222222;font-weight: bold;}
.index-cubeTitle>view .more-icon{width: 10rpx;height: 18rpx;}
.index-cubeTitle>view.more-msg{font-size:26rpx;color:#555555;}
.index-cubeInfo{width: 96%;margin:20rpx 2% 0;background-color: #ffffff;border-radius: 10rpx;height: 400rpx;margin-top: -60rpx;}
.index-cubeInfo-1 image:nth-child(1){width: calc(96%);padding: 20rpx 2%;float: left;height: 360rpx;}
.index-cubeInfo-2 image:nth-child(1){width: calc(46% - 1rpx);padding: 20rpx 2%;float: left;height: 360rpx;border-right: 1px solid #e8e8e8;}
.index-cubeInfo-2 image:nth-child(2){width: calc(46% - 1rpx);padding: 20rpx 2%;float: left;height: 360rpx;border-bottom: 1px solid #e8e8e8;}
.index-cubeInfo-3{width: 96%;margin:20rpx 2% 0;background-color: #ffffff;border-radius: 10rpx;height: 400rpx;margin-top: -60rpx;}
.index-cubeInfo-3 image:nth-child(1){width: 46%;padding: 20rpx 2%;float: left;height: 360rpx;border-right: 1px solid #e8e8e8;}
.index-cubeInfo-3 image:nth-child(2){width: 45%;padding: 20rpx 2%;float: left;height: 160rpx;border-bottom: 1px solid #e8e8e8;}
.index-cubeInfo-3 image:nth-child(3){width: 45%;padding: 20rpx 2%;float: left;height: 160rpx;}
.index-cubeInfo-4{width: 96%;margin:20rpx 2% 0;background-color: #ffffff;border-radius: 10rpx;height: 400rpx;margin-top: -60rpx;}
.index-cubeInfo-4 image:nth-child(1){width: 45%;padding: 20rpx 2%;float: left;height: 360rpx;border-right: 1px solid #e8e8e8;}
.index-cubeInfo-4 image:nth-child(2){width: 46%;padding: 20rpx 2%;float: left;height: 160rpx;border-bottom: 1px solid #e8e8e8;}
.index-cubeInfo-4 image:nth-child(3){width: 20%;padding: 20rpx 2%;float: left;height: 160rpx;border-right: 1px solid #e8e8e8;}
.index-cubeInfo-4 image:nth-child(4){width: 20%;padding: 20rpx 2%;float: left;height: 160rpx;}
/* 热销商品 */
.shop-list{width: 96%;padding:20rpx 2%;padding-bottom: 170rpx;}
.shop-list  > text{text-align: center;position: relative;display: block;width: 100%;margin: 10rpx 0;font-size: 32rpx}
.shop-list  > text::before{content: '';position: absolute;left: 150rpx;top: 24rpx;width: 100rpx;height: 2rpx;background-color: #e8e8e8;}
.shop-list  > text::after{content: '';position: absolute;right: 150rpx;top: 24rpx;width: 100rpx;height: 2rpx;background-color: #e8e8e8;}
.shop-list-block{width: 44%;margin: 10rpx 1%;padding: 20rpx 2%;float: left;background-color: #ffffff;border-radius: 10rpx;}
.shop-list-block image{width: 100%;height: 300rpx;}
.shop-list-block .list-block-title{display: block;text-align: left;height: 40rpx;line-height: 40rpx;}
.shop-list-block .list-block-title{font-size: 28rpx;color: #777777;overflow: hidden;text-overflow: ellipsis;height: 85rpx;-webkit-line-clamp: 2;line-clamp:2;display: -webkit-box;-webkit-box-orient: vertical;}
.shop-list-block .list-block-marketprice{font-size: 24rpx;color: #ea4149}

.tabbar{width: 100%;position: fixed;bottom: 0;background-color: #F7F7FA;border-top: 1px solid #e8e8e8;padding: 10rpx 0;}
.tabbar-block{width: 25%;float: left;text-align: center;}
.tabbar-active{color: #ea4149;}
.tabbar-block image{width: 56rpx;height: 56rpx;}
.tabbar-block text{display: block;width: 100%;text-align: center;font-size: 20rpx;margin-top: -6rpx;}
.tabbar-block-cart{position: relative;}

.isIpx{padding: 10rpx 0 68rpx;}

.mask{width: 100%;height: 100%;position: fixed;top: 0;left: 0;background: #000000;z-index: 20;opacity: 0.7;}
.modalDlg{position: fixed;top: 15%;left: 0;z-index: 21;width: 100%;display: -webkit-box;display: -webkit-flex;display: flex;flex-direction: column;align-items: center;}
.modalDlg image:nth-child(1){width: 90%;height: 670rpx;}
.modalDlg image:nth-child(2){width: 56rpx;height: 56rpx;margin-top: 50rpx;}
.modalDlgInfo{width: 65%;margin-top:-460rpx;}
.modalDlgInfoText1{width: 100%;float: left;text-align: center;}
.modalDlgInfoText1 text:nth-child(1){font-size: 24rpx;color: #da5d3d;line-height: 90rpx;}
.modalDlgInfoText1 text:nth-child(2){font-size: 70rpx;color: #da5d3d;line-height: 90rpx;margin-top: -8rpx;}
.modalDlgInfoText1 text:nth-child(3){font-size: 20rpx;color:#878986;display:block;width: 100%;float: left;text-align: center;}
/* .modalDlgInfoText2{width: 65%;float: left;margin-left: 5%;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.modalDlgInfoText2 text:nth-child(1){font-size: 36rpx;line-height: 90rpx;display: block;}
.modalDlgInfoText2 text:nth-child(2){font-size: 20rpx;color:#878986;display:block;width: 100%} */
.modalButton{margin-top: 130rpx;}
.modalButton text{display: block;width: 300rpx;background-color:#f8d7a1;color: #59442f;font-size: 36rpx;font-weight: bold;text-align: center;padding: 10rpx 0;border-radius: 5rpx;}
.slogan{padding:0rpx 10rpx;color:#ea4149;font-size:28rpx;border:1px solid #ea4149;border-radius:8rpx;margin-right:4rpx;}

/*0元购入口图标*/
.bargain-logo{position: fixed;right:0;top:600rpx;width:200rpx;height: 200rpx;}
/* 购物车图标 */
.shop-logo{position: fixed;right:20rpx;top:900rpx;width:80rpx;height: 80rpx;background-color: #777777;opacity: 0.8;border-radius: 50%;text-align: center;}
.shop-logo image{width: 46rpx;margin-top: 18rpx;}
.shop-logo text:nth-child(2){opacity: 0.9;display: block;position: absolute;top: -10rpx;right: 0;background-color: red;border-radius: 50%;font-size: 24rpx;width: 30rpx;color: #ffffff;}
/*文章收藏页入口*/
.article-logo{left:0;width:150rpx;height:150rpx;}
/* 顶部提示 */
.addTips{position: fixed;top: 20rpx;right: 10rpx;display: block;background-color:rgba(0,0,0,0.7);z-index: 50;padding:10rpx 20rpx;height: 60rpx;line-height: 50rpx;border-radius: 6px;}
.addTips text{color: #ffffff;font-size: 26rpx;}
.addTips text:nth-child(2){margin-left: 10rpx;}
.addTips text:nth-child(3){margin-left: 10rpx;}
.addTips-three{position: fixed;top: 0;right: 120rpx;z-index: 50;}
.addTips-three-block{width:0;height:0;border-width:0 10px 10px;border-style:solid;border-color:transparent transparent #000;position:relative;opacity: 0.7;}


/* 名庄名酒标签样式 */
.index-wine-tips{width: 100%;background-color: #ffffff;line-height: 80rpx;height: 80rpx;}
.index-wine-tips-block{width: 33.3%;float: left;text-align: center;}
.index-wine-tips-block image{width: 40rpx;margin-bottom: -8rpx;margin-left: 4rpx;}
.index-wine-tips-block text{font-size:28rpx;color:#ea4149;margin-left: 6rpx;}

/*推荐商品底部显示没有更多了*/
.no-more-product-msg{font-size:28rpx;text-align: center;color:#222222;clear:both;line-height:80rpx;}

/*回到顶部*/
.back-to-top{width:60rpx;height:54rpx;position: fixed;bottom:150rpx;right:30rpx;}



