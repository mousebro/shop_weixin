/* pages/bargain-comfirm/index.wxss */
page{background:#f2f2f2}

.header-container{position: relative;width:100%;background: #fff;overflow: hidden;height: 1060rpx;}
.header-container-noOrder{height:1060rpx;}

.header-bg{height:400rpx;width: 100%;background: linear-gradient(90deg,#e94149,#fe6749);position: relative;}
.header-bg:after{display: block;content:"";border-top:200rpx solid transparent;border-left: 200rpx solid #fff;border-bottom:200rpx solid transparent;border-right: 200rpx solid transparent;width:0rpx;height: 0;position: absolute;top:300rpx;}
.header-bg::before{display: block;content:"";border-top:200rpx solid transparent;border-left: 200rpx solid transparent;border-bottom:200rpx solid transparent;border-right: 200rpx solid #fff;width:0rpx;height: 0;position: absolute;top:300rpx;right:0}
.header-container>.card{width:calc(100% - 40rpx);position: absolute;top:150rpx;left:20rpx;z-index: 10;background: #fff;box-shadow: 0px 4rpx 15rpx 2rpx #eee;padding-top:30rpx;}
.header-container>.card-position{top:150rpx;}
.header-container>.card>.card-contain{width: 100%;height: 100%;display: flex;flex-direction: column;align-items: center;padding-top:40rpx;position: relative;}
.header-container>.card>.card-contain>.avatar{width: 120rpx;height: 120rpx;border-radius: 100rpx;position: absolute;left:calc(50% - 70rpx);top:-100rpx;border:10rpx solid #fff;}
.header-container>.card>.card-contain>.customer{font-size:32rpx;color:#222222;margin:10rpx 0 ;}
.header-container>.card>.card-contain>.msg{font-size:28rpx;color:#777777;width: 80%;display: inline-block;text-align: center;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;margin-bottom: 20rpx;}
.header-container>.card>.card-contain>.product-card{padding:20rpx;background: #f2f2f2;display: flex;width: calc(100% - 80rpx);height: 170rpx;}
.header-container>.card>.card-contain>.product-card>.product-img{width:170rpx;height: 170rpx; }
.header-container>.card>.card-contain>.product-card>.product-decr{width: calc(100% - 200rpx);height: 100%;display: flex;flex-direction: column;justify-content:space-around;margin-left:20rpx; }
.header-container>.card>.card-contain>.product-card>.product-decr>view:first-child{width: 100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;font-size:32rpx;color:#222222; }
.header-container>.card>.card-contain>.product-card>.product-decr>view.price>text:first-child{color:#ea4149;font-size:28rpx;display: inline-block;margin-right: 10rpx;}
.header-container>.card>.card-contain>.product-card>.product-decr>view.price>text:last-child{color:#777777;font-size:20rpx;text-decoration: line-through;}

.shop-swiper{width: 100%;height: 700rpx;position:relative;z-index: 2;overflow: hidden;}
.swiper-img{width: 100%;height: 100%;}
.swiper-img-avatar{width: 40rpx;height: 40rpx;border-radius:60rpx;margin-right: 10rpx;}
.shop-swiper-custom{height:40rpx;}
.shop-swiper-item{width: 35%;}
.shop-swiper-custom view{display: flex;align-items: center;width:100%;font-size:24rpx;color:#777777;height:100%;}
.buy-button{position: absolute;bottom:40rpx;right: 40rpx;font-size:28rpx;padding:6rpx 20rpx;background:linear-gradient(90deg,#e94149,#fe6749);color:#fff;border-radius: 4rpx;}

.progress-container{padding:20rpx;}
.progress-container>text{text-align: center;display: inline-block;width: 100%;font-size:34rpx;color:#ea4149;margin-bottom: 20rpx;}
.progress-container>.target{display: flex;justify-content: space-between;font-size:32rpx;color:#222222;padding:20rpx 0; }

.team-container{margin-top:365rpx;background: #fff;padding:40rpx;}
.team-container .team-msg{text-align: center;color:#ea4149;margin-bottom:20rpx;}
.team-container .team-msg::before{display:inline-block;margin-right:4%;width: 20%;content: "";height: 0;border-top: 1px solid #ea4149;vertical-align: middle;} 
.team-container .team-msg::after{display:inline-block;margin-left:4%;width: 20%;content: "";height: 0;border-top: 1px solid #ea4149;vertical-align: middle;}
.team-container .team-msg-two>view:first-child{font-size: 30rpx;text-align: center;color:#222222;}
.team-container .team-msg-two>view>text{color:#ea4149;}
.team-container .team-msg-two>view:nth-child(2){text-align: center;font-size:28rpx;color:#222222;}
.team-container .team-msg-two>view:nth-child(2)>text{color:#fff;background:#222222;padding:6rpx 10rpx;font-size:34rpx;margin:10rpx;border-radius:5rpx;}
.share-btn{background:linear-gradient(to right,#ff6849,#ea4149); font-size:30rpx;color:#ffffff;text-align: center;box-sizing: border-box;padding: 30rpx 45rpx;border-radius:10rpx;margin:50rpx auto 0;}
.share-btn-animation{width:85%;animation: free_download 1s linear alternate infinite;-webkit-animation: free_download 1s linear alternate infinite;padding: 25rpx 45rpx;}
@-webkit-keyframes free_download{0%{-webkit-transform:scale(1);}100%{-webkit-transform:scale(1.1);}}
@keyframes free_download{0%{transform:scale(1);}100%{transform:scale(1.1);}}
.team-container>.btn-class:first-child{margin: 0;padding:30rpx 50rpx;animation: free_download 1s linear alternate infinite;-webkit-animation: free_download 1s linear alternate infinite;padding: }

.team-container>.btn-class:last-child{margin-top:20rpx;padding:25rpx 50rpx;background: transparent;color:#ea4149;border:1px solid #ea4149;}

.helper-container{margin-top:20rpx;padding:20rpx;background: #fff;}
.helper-container>.msg{display: inline-block;width:100%;text-align: center;font-size:32rpx;color:#222222;position: relative;}
.helper-container  > .msg{text-align: center;position: relative;display: block;width: 100%;margin: 10rpx 0;font-size: 32rpx}
.helper-container  > .msg::before{content: '';position: absolute;left: 150rpx;top: 24rpx;width: 100rpx;height: 2rpx;background-color: #c3c3c3;}
.helper-container  > .msg::after{content: '';position: absolute;right: 150rpx;top: 24rpx;width: 100rpx;height: 2rpx;background-color: #c3c3c3;}

.helper-container>view{display: flex;justify-content: space-between;padding: 20rpx 0;}
.helper-container>view:not(:last-child){border-bottom: 1px solid #e8e8e8;}

.helper-container>view>.left-side{width: 50%;display: flex;align-items: center;}
.helper-container>view>.left-side>.avatar{height:60rpx;width: 60rpx;border-radius: 60rpx;margin-right: 10rpx;}
.helper-container>view>.left-side>.helper-name{font-size:28rpx;color:#222222;}
.helper-container>view>.right-side{font-size:28rpx;color:#ea4149;display: flex;width:30%;justify-content: flex-end;align-items: center;}
.noMsg-container {align-items: center;flex-direction: column;font-size:28rpx;color:#777777;}


.fixed-header{position: absolute;top:0;left: 0;display: flex;justify-content: space-between;width: 100%;overflow: hidden;height:150rpx;align-items: center;font-size:28rpx;color:#fff;}
.fixed-header>text{background:rgba(255,255,255,.2);border-radius:20rpx;}
.fixed-header>text:first-child{margin-left: -20rpx;padding:4rpx 20rpx 4rpx 40rpx;}
.fixed-header>text:last-child{margin-right: -20rpx;padding:4rpx 40rpx 4rpx 20rpx;}
.fixed-header>view:first-child>text:not(:nth-child(2)):not(:nth-child(4)){padding:8rpx 12rpx;background: #222222;border-radius: 4rpx;}
.fixed-header>view:first-child>text:nth-child(2),text:nth-child(4){margin:0 10rpx;color:#222222;}


/*遮罩层*/

.mask{width: 100%;height: 100%;position: fixed;top: 0;left: 0;background: #000000;z-index: 20;opacity: 0.7;}
.modal1{position: fixed;z-index: 21;width: 100%;height: 100%;text-align: center;top:0;left:0;right:0;bottom:0;}
.modal1 >view{width: 90%;position: absolute;top:0;left: 0;right:0;bottom: 0;margin: auto;height: 1170rpx;}
.modal1 >view> text{position: relative;z-index: 25;display: block;margin-top: 100rpx;font-size: 32rpx;color: #222222;font-weight: bold;}
.modal1 >view> text:before{content: '';position: absolute;width: 10%;height: 1rpx;background-color: #d6d6d6;top: 22rpx;left: 26%;}
.modal1 >view> text:after{content: '';position: absolute;width: 10%;height: 1rpx;background-color: #d6d6d6;top: 22rpx;right: 26%;}
.modal1 >view> image{position: absolute;width: 100%;top: 40rpx;left: 0;}
.modalRules{position: absolute;width: 100%;top: 150rpx;z-index: 23;text-align: left;height: 970rpx;box-sizing: border-box;}
.modalRules text{font-size: 24rpx;color: #777777;}
.modalRules-scroll{width: 90%;height: 100%;padding:0 5%;}
/*隐藏scroll-view的滚动条*/
.allcontainer::-webkit-scrollbar{ width: 0;height: 0; color: transparent;}

.close{position: absolute;z-index: 26;top: 70rpx;right: 70rpx;}
.close image{width: 30rpx;height: 30rpx;}

.card-product-img{width: 300rpx;height:300rpx;}
.card-contain>.title{font-size:32rpx;color:#222222;width: 80%;display: inline-block;text-align: center;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;margin-top: 20rpx;}

.bargain-over-msg{font-size:32rpx;color:#222222;text-align:center; }

/* pages/bargain-detail/index.wxss */
/*遮罩层*/

.mask{width: 100%;height: 100%;position: fixed;top: 0;left: 0;background: #000000;z-index:20;opacity: 0.6;}
/*海报*/
.poster{z-index:21;position: fixed;}
.poster-close-btn{height:60rpx;width:60rpx;top:40rpx;right:20rpx;}
.poster-container{height: calc(100% - 300rpx);width: calc(100% - 80rpx);top:110rpx;left:20rpx;background: #ffffff;padding:40rpx 20rpx;overflow: hidden;border-radius: 10rpx;}
.poster-save-btn{color:#ffffff;padding:18rpx calc(30% - 40rpx);display: inline-block;font-size:40rpx;background:#fc6349;border-radius:40rpx;line-height: 40rpx;}
.poster-btn-container{display: flex;justify-content: center;bottom:20rpx;width: 100%;height: 76rpx;}
/*隐藏滚动条*/
.scroll-view{height:100%;}
::-webkit-scrollbar{width:0;height: 0;color:transparent;}
/*分享者信息*/
.poster-container .sharer-msg{display: flex;justify-content: space-between;width:100%;overflow: hidden;align-items: center;}
.poster-container .sharer-msg>.share-avatar{display: flex;align-items: center;width:60%;}
.poster-container .sharer-msg>.share-avatar>.avatar{width:80rpx;height:80rpx;border-radius:40rpx;}
.poster-container .sharer-msg>.share-avatar>.share-name{font-size:32rpx;color:#222222;margin-left:10rpx;overflow: hidden;white-space: nowrap;text-overflow:ellipsis;}
.poster-container .sharer-msg>.logo{height:60rpx;width:20%;}
.poster-container .slogan{font-size:30rpx;color:#777777;margin-top:10rpx;overflow: hidden;white-space: nowrap;text-overflow:ellipsis;}
.poster-container .product-img{width:100%;margin-top:40rpx;}
.poster-container .product-title{display:-webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp:2;overflow: hidden;text-overflow:ellipsis;font-size:32rpx;color:#222222;font-weight:500;}

.poster-container .price-qr-container{display: flex;width:100%;justify-content: space-between;}
.poster-container .price-qr-container>.left-price{width:50%;margin-top:20rpx;}
.poster-container .price-qr-container>.left-price>view:first-child>text{font-size:24rpx;padding:4rpx 10rpx;border:1px solid #ea4149;color:#ea4149;margin-right:10rpx;border-radius:4rpx;}
.poster-container .price-qr-container>.left-price>view:first-child>text:nth-child(2){border:1px solid #ff8046;color:#ff8046;}

.poster-container .price-qr-container>.left-price>view.price>.icon{font-size:40rpx;color:#ea4149;}
.poster-container .price-qr-container>.left-price>view.price>.price-a{font-size:80rpx;color:#ea4149;}
.poster-container .price-qr-container>.left-price>view.price>.price-d{font-size:40rpx;color:#ea4149;}
.poster-container .price-qr-container>.left-price>view.del-price{text-decoration:line-through;color:#777777;font-size:26rpx;}
.poster-container .price-qr-container>.right-QR{width:35%;display: flex;justify-content: center;align-items: center;flex-direction: column;}
.poster-container .price-qr-container>.right-QR>.QR{width: 140rpx;height: 140rpx;margin-top:20rpx;}
.poster-container .price-qr-container>.right-QR>view{font-size:24rpx;}

.canvas{width: 100%;height:1rpx;overflow: hidden;position: fixed;left:10000rpx;}

/* 顶部提示 */
.addTips{position: fixed;top: 20rpx;right: 10rpx;display: block;background-color:rgba(0,0,0,0.7);z-index: 50;padding:10rpx 20rpx;height: 60rpx;line-height: 50rpx;border-radius: 6px;text-align: center;}
.addTips text{color: #ffffff;font-size: 26rpx;}
.addTips text:nth-child(2){margin-left: 10rpx;}
.addTips text:nth-child(3){margin-left: 10rpx;}
.addTips-three{position: fixed;top: 0;right: 120rpx;z-index: 50;}
.addTips-three-block{width:0;height:0;border-width:0 10px 10px;border-style:solid;border-color:transparent transparent #000;position:relative;opacity: 0.7;}

/*没有帮砍的图标提示*/
.noMsg-container{width: 100%;}
.noMsg-img{width: 300rpx;height: 300rpx;margin: 0 auto;}

.sheet-container{background: transparent;width: 100%;}
.sheet-item{background: transparent;border-radius: 10rpx;font-size:36rpx;}
.sheet-button{background: transparent;font-size:36rpx;border:none;}


.toast-container{position: fixed;top:0;left: 0;right: 0;bottom: 0;margin: auto;z-index:30;width: 100%;height: 100%;overflow: hidden;}
.toast-container>view{height: 650rpx;width: 600rpx;position:fixed;top:0;left: 0;right: 0;bottom: 0;margin:auto;}
.toast-container>view>view.msg{display: flex;width: 100%;height: 100%;flex-direction: column;align-items: center;font-size:44rpx;color:#ea4149;}
.toast-container>view>view.msg>text{font-size:44rpx;}
.toast-container>view>view.msg>text:first-child{margin-top:300rpx;margin-bottom: 22rpx;}
.toast-container>view>view.msg>text.buy-btn{font-size:38rpx;padding:25rpx 100rpx;background: linear-gradient(to bottom right,#ea4149,#fb8469);color:#fff;border-radius: 80rpx;margin-top:40rpx;box-shadow: 0 5rpx 0 5rpx #fee0e0;}
.toast-container>view>view.close-container{position: relative}
.toast-container>view>view.close-container>.close-btn{position: absolute;width: 70rpx;height: 70rpx;top:-50rpx;right:-30rpx;}

.waitingPaint{background: #343434 !important;opacity: 0.7 !important;}
.btn{ width: 50%; height: 200rpx; margin: 0; padding: 0; border-radius: 0; position: absolute; background: transparent; top: 1; left: 0; z-index: 9;}
button::after{
  border:none;
}
.aa{
  width: 50%;
  height: 200rpx;
  background: transparent;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}

