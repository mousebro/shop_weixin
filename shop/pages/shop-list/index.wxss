/* 搜索框 */
.search{width: 90%;background-color: #f2f2f2;border-radius: 40rpx;line-height: 60rpx;margin:28rpx 5% 0;box-sizing: border-box;}
.search image{width: 40rpx;height: 40rpx;margin-left: 20rpx;display: inline-block;margin-bottom: -10rpx;}
.search text{font-size: 24rpx;margin-left: 10rpx;color: #777777;display: inline-block;}
.headerTabs{ display: flex;justify-content: space-between;padding:36rpx 117rpx;font-size: 24rpx;border-bottom: 1px solid #f2f2f2;box-sizing: border-box;}
.headerTabs image{width: 18rpx;height: 22rpx;vertical-align: middle;margin-left: 10rpx;}
.headerTabs view.isActive{color:#ea4149;}

/*隐藏scroll-view的滚动条*/
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
/*商品列表*/
.productList{padding: 25rpx;}
.productList>view{display:flex;padding:25rpx 50rpx;border-bottom: 1rpx solid #f2f2f2;}
.productList .productSubscrib{padding: 0rpx 0 0 40rpx;display: flex;flex-direction: column;justify-content: center;}
.productList .productSubscrib>text{font-size:32rpx;}
.productSubscrib>view:first-child{display: inline-block;width: 450rpx;overflow: hidden;text-overflow:ellipsis;font-weight: 600;color:#222222;font-size:32rpx;white-space: nowrap; }
.productList .productSubscrib>view>text:first-child{font-size:28rpx;color:#ea4149;display: inline-block;margin-right:10rpx;}
.productList .productSubscrib>view>text:last-child{font-size:24rpx;color:#777777;text-decoration:line-through;}
.productList>view>view>image{width: 110rpx;height: 125rpx;}
